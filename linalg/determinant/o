/showuline {
	/CurPointSize exch def
	dup stringwidth pop
	gsave
	currentfont /FontInfo get dup
	/UnderlinePosition get CurPointSize mul 1000 div 0 exch rmoveto exch 0 rlineto
	/UnderlineThickness get CurPointSize mul 1000 div setlinewidth stroke
	grestore
	show
} def
/mv {moveto} def
